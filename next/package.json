{
  "name": "eligundry.com",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.0",
    "@mdx-js/react": "^2.1.0",
    "@next/mdx": "^12.1.0",
    "axios": "^0.26.1",
    "chart.js": "^3.7.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "chartjs-chart-sankey": "^0.8.1",
    "date-fns": "^2.28.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^4.0.0",
    "next-transpile-modules": "^9.0.0",
    "postscribe": "^2.0.8",
    "react": "17.0.2",
    "react-chartjs-2": "^3.3.0",
    "react-dom": "17.0.2",
    "react-github-calendar": "^3.3.1",
    "react-icons": "^4.3.1",
    "react-lazyload": "^3.2.0",
    "react-lite-yt-embed": "^1.2.7",
    "react-loading-skeleton": "^3.0.3",
    "react-tooltip": "^4.2.21",
    "react-use": "^17.3.2",
    "rehype-img-size": "^1.0.1",
    "styled-components": "^5.3.3",
    "tailwindcss": "^2.2.19",
    "twin.macro": "^2.8.2",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.180",
    "@types/node": "17.0.23",
    "@types/react": "17.0.42",
    "@types/styled-components": "^5.1.24",
    "@types/tailwindcss": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "typescript": "4.6.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "styled-components",
      "debugPlugins": false,
      "debug": false
    }
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not IE 11",
    "not ie_mob 11"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "next",
      "next/core-web-vitals",
      "prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "ignorePatterns": [
      "public",
      "static",
      ".cache",
      "content",
      "*.d.ts"
    ],
    "rules": {
      "react/prefer-stateless-function": "off",
      "react/prop-types": "off",
      "react/no-danger": "off",
      "react/destructuring-assignment": "off",
      "react/no-unescaped-entities": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-spreading": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-use-before-define": "off",
      "react/require-default-props": "off",
      "no-param-reassign": "off",
      "radix": "off",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight",
            "to"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".tsx",
            ".jsx"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "mjs": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": [
            "arrow-function"
          ]
        }
      ]
    },
    "settings": {
      "import/core-modules": [],
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true
    }
  }
}
