{
  "name": "eligundry.com",
  "description": "Eli Gundry's personal website",
  "version": "3.0.0",
  "author": "Eli Gundry <eligundry@gmail.com>",
  "repository": "https://github.com/eligundry/eligundry.com",
  "dependencies": {
    "@eligundry/gatsby-source-goodreads": "^1.0.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "chart.js": "^3.6.0",
    "chartjs-adapter-date-fns": "^2.0.0",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "gatsby": "^4.2.0",
    "gatsby-plugin-catch-links": "^4.2.0",
    "gatsby-plugin-feed": "^4.2.0",
    "gatsby-plugin-google-tagmanager": "^4.2.0",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-lodash": "^5.2.0",
    "gatsby-plugin-mdx": "^3.2.0",
    "gatsby-plugin-netlify": "^4.0.0-next.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sass": "^5.2.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "gatsby-plugin-styled-components": "^5.2.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-plugin-twitter": "^4.2.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^4.2.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^5.2.0",
    "gatsby-remark-images": "^6.2.0",
    "gatsby-remark-prismjs": "^6.2.0",
    "gatsby-remark-responsive-iframe": "^5.2.0",
    "gatsby-source-custom-api": "^2.3.2",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-source-lastfm": "^0.1.2",
    "gatsby-transformer-sharp": "^4.2.0",
    "lodash": "^4.17.21",
    "postscribe": "^2.0.8",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-github-calendar": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-lazyload": "^3.2.0",
    "react-loading-skeleton": "^3.0.1",
    "react-schemaorg": "^2.0.0",
    "react-tooltip": "^4.2.21",
    "react-use": "^17.3.1",
    "sass": "^1.43.4",
    "schema-dts": "^1.0.0",
    "simple-git": "^2.47.0",
    "styled-components": "^5.3.3",
    "tailwindcss": "^2.2.19",
    "twin.macro": "^2.8.1",
    "typescript": "^4.5.2",
    "url-join-ts": "^1.0.5",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/react-helmet": "^6.1.4",
    "@types/react-lazyload": "^3.1.1",
    "@types/styled-components": "^5.1.15",
    "@types/tailwindcss": "^2.2.4",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop --host 0.0.0.0",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format:js": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "type-check": "tsc --noEmit"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "babelMacros": {
    "twin": {
      "config": "src/tailwind.config.js",
      "preset": "styled-components",
      "autoCssProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not IE 11",
    "not ie_mob 11"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "ignorePatterns": [
      "public",
      "static",
      ".cache",
      "content"
    ],
    "rules": {
      "react/prefer-stateless-function": "off",
      "react/prop-types": "off",
      "react/no-danger": "off",
      "react/destructuring-assignment": "off",
      "react/no-unescaped-entities": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-spreading": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-use-before-define": "off",
      "react/require-default-props": "off",
      "no-param-reassign": "off",
      "radix": "off",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight",
            "to"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".tsx",
            ".jsx"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "mjs": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    },
    "settings": {
      "import/core-modules": [],
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true
    }
  }
}
