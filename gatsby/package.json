{
  "name": "eligundry.com",
  "description": "GatsbyJS starter that includes examples for advanced use cases.",
  "version": "1.1.0",
  "author": "Eli Gundry <eligundry@gmail.com>",
  "repository": "https://github.com/eligundry/eligundry.com",
  "dependencies": {
    "date-fns": "^2.9.0",
    "gatsby": "^2.18.18",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-catch-links": "^2.1.21",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-graphql-codegen": "^2.2.1",
    "gatsby-plugin-lodash": "^3.1.18",
    "gatsby-plugin-nprogress": "^2.1.17",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-twitter": "^2.1.17",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-autolink-headers": "^2.1.22",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-remark-prismjs": "^3.3.29",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-remark": "^2.6.45",
    "gatsby-transformer-sharp": "^2.3.9",
    "gravatar": "^1.8.0",
    "lodash": "^4.17.15",
    "prismjs": "^1.18.0",
    "react": "^16.10.1",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.10.1",
    "react-fetch-hook": "^1.7.1",
    "react-github-calendar": "^0.14.1",
    "react-goodreads": "^1.0.7",
    "react-goodreads-shelf": "^1.0.3",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-lazyload": "^2.6.5",
    "react-tooltip": "^4.0.3",
    "react-twitter-embed": "^3.0.3",
    "react-twitter-widgets": "^1.7.1",
    "react-use": "^13.21.0",
    "rehype-react": "^4.0.1",
    "styled-components": "^5.0.0",
    "twin.macro": "^1.7.0",
    "url-join": "^4.0.0",
    "use-detect-print": "0.0.2"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "babel-plugin-styled-components": "^1.10.6",
    "cli-glob": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gh-pages": "^2.2.0",
    "prettier": "^1.17.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.2",
    "source-map-explorer": "^2.3.1",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "typescript": "^3.7.5",
    "typescript-eslint-parser": "^22.0.0",
    "write-good": "^1.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop --host 0.0.0.0",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "stats:html": "source-map-explorer public/*.js --html public/stats.html"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "babelMacros": {
    "twin": {
      "config": "src/tailwind.config.js",
      "preset": "styled-components",
      "autoCssProp": true,
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  }
}
