  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop --host 0.0.0.0",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh-actions": "npm run build:pp",
    "deploy": "gatsby-plugin-s3 deploy",
    "clean": "gatsby clean",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "stats:html": "source-map-explorer public/*.js --html public/stats.html",
    "type-check": "tsc --noEmit"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "babelMacros": {
    "twin": {
      "config": "src/tailwind.config.js",
      "preset": "styled-components",
      "autoCssProp": true,
      "debugPlugins": false,
      "debug": false
    }
  }
