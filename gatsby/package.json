{
  "name": "eligundry.com",
  "description": "Eli Gundry's personal website",
  "version": "3.0.0",
  "author": "Eli Gundry <eligundry@gmail.com>",
  "repository": "https://github.com/eligundry/eligundry.com",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "axios": "^0.21.1",
    "chart.js": "^3.3.2",
    "chartjs-adapter-date-fns": "^1.1.0-beta.1",
    "date-fns": "^2.22.1",
    "dotenv": "^8.6.0",
    "gatsby": "^3.7.2",
    "gatsby-plugin-catch-links": "^3.7.1",
    "gatsby-plugin-feed": "^3.7.1",
    "gatsby-plugin-google-tagmanager": "^3.7.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-lodash": "^4.7.1",
    "gatsby-plugin-mdx": "^2.8.0",
    "gatsby-plugin-nprogress": "^3.7.1",
    "gatsby-plugin-postcss": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-remove-trailing-slashes": "^3.7.1",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-styled-components": "^4.7.1",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-plugin-twitter": "^3.7.1",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.7.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^4.4.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-remark-prismjs": "^5.4.1",
    "gatsby-remark-responsive-iframe": "^4.4.1",
    "gatsby-source-custom-api": "^2.1.4",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-source-lastfm": "^0.1.2",
    "gatsby-transformer-sharp": "^3.7.1",
    "jsdom": "^17.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.3.0",
    "postscribe": "^2.0.8",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-github-calendar": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-image-lightbox": "^5.1.1",
    "react-lazy-load-image-component": "^1.5.0",
    "react-lazyload": "^3.2.0",
    "react-schemaorg": "^1.3.1",
    "react-tooltip": "^4.2.21",
    "react-twitter-embed": "^3.0.3",
    "react-twitter-widgets": "^1.9.5",
    "react-use": "^17.3.1",
    "sass": "^1.42.1",
    "schema-dts": "^0.8.2",
    "simple-git": "^2.40.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.2.15",
    "twin.macro": "^2.8.0",
    "typescript": "^4.2.4",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.10",
    "@types/lodash": "^4.14.170",
    "@types/react-helmet": "^6.1.2",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/react-lazyload": "^3.1.0",
    "@types/styled-components": "^5.1.9",
    "@types/tailwindcss": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "cli-glob": "^0.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^1.17.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "source-map-explorer": "^2.5.0",
    "typescript-eslint-parser": "^22.0.0",
    "write-good": "^1.0.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop --host 0.0.0.0",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh-actions": "npm run build:pp",
    "deploy": "gatsby-plugin-s3 deploy",
    "clean": "gatsby clean",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:md": "remark content/",
    "write-good": "write-good $(glob 'content/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "stats:html": "source-map-explorer public/*.js --html public/stats.html",
    "type-check": "tsc --noEmit"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "babelMacros": {
    "twin": {
      "config": "src/tailwind.config.js",
      "preset": "styled-components",
      "autoCssProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not IE 11",
    "not ie_mob 11"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "ignorePatterns": [
      "public",
      "static",
      ".cache",
      "content"
    ],
    "rules": {
      "react/prefer-stateless-function": "off",
      "react/prop-types": "off",
      "react/no-danger": "off",
      "react/destructuring-assignment": "off",
      "react/no-unescaped-entities": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-spreading": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-use-before-define": "off",
      "react/require-default-props": "off",
      "no-param-reassign": "off",
      "radix": "off",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight",
            "to"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".tsx",
            ".jsx"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "mjs": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    },
    "settings": {
      "import/core-modules": [],
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true
    }
  }
}
