<header class="navbar">
  <div class="container mx-auto flex flex-wrap">
    <div class="flex-1">
      <a href="/" itemprop="sameAs" class="btn btn-ghost normal-case text-xl site-name">Eli Gundry</a>
    </div>
    <nav class="flex-none">
      <ul class="menu menu-horizontal p-0">
        {% for link in topNav %}
          <li>
            <a href="{{ link.href }}"{% if page.url == link.href %} aria-current="page"{% endif %}>
              {% emojiText link.emoji, link.emojiLabel, link.name %}
            </a>
          </li>
        {% endfor %}
        <li>
          <is-land autoinit="preact" import="/components/ThemeChanger.js"></is-land>
        </li>
      </ul>
    </nav>
  </div>
</header>
