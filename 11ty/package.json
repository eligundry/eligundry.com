{
  "name": "@eligundry/eligundry.com",
  "version": "5.0.0",
  "description": "My personal site with 11ty.dev",
  "main": "index.js",
  "scripts": {
    "start": "run-p 'watch:*'",
    "watch:11ty": "eleventy --serve",
    "watch:tailwind": "npm run build:tailwind -- --watch",
    "build": "run-s build:tailwind build:11ty",
    "build:11ty": "eleventy",
    "build:tailwind": "tailwindcss -i ./css/tailwind.css -o ./_site/css/style.css --postcss postcss.config.js"
  },
  "author": "Eli Gundry <eligundry@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@tailwindcss/typography": "^0.5.7",
    "@types/async-retry": "^1.4.5",
    "async-retry": "^1.3.3",
    "daisyui": "^2.27.0",
    "date-fns": "^2.29.2",
    "dotenv": "^16.0.2",
    "jsdom": "^20.0.0",
    "lastfm-typed": "^2.0.2",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-highlightjs": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8"
  }
}
